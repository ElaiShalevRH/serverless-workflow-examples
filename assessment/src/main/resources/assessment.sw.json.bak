{
    "id": "assessment",
    "version": "1.0",
    "specVersion": "0.8",
    "name": "Assessment Workflow example",
    "description": "An example of assessment workflow",
    "start": "Hello",
    "extensions": [
        {
            "extensionid": "workflow-output-schema",
            "outputSchema": "schema/workflow-options.json"
        }
    ],
    "functions": [
        {
            "name": "wfCollection",
            "operation": "rest:get:http://localhost:3005/workflowoptions",
            "type": "custom"
        },
        {
            "name": "debug",
            "type": "custom",
            "operation": "sysout"
        }
    ],
    "states": [
        {
            "name": "Hello",
            "type": "operation",
            "actionMode": "sequential",
            "actions": [
                {
                    "name": "printAction",
                    "functionRef": {
                        "refName": "debug",
                        "arguments": {
                            "message": "Hello"
                        }
                    }
                }
            ],
            "transition": "Start"
        },
        {
            "name": "Start",
            "type": "operation",
            "actions": [
                {
                    "name": "getWfCollection",
                    "functionRef": "wfCollection"
                }
            ],
            "transition": "Hello2"
        },
        {
          "name": "Hello2",
          "type": "operation",
          "actionMode": "sequential",
          "actions": [
              {
                  "name": "printAction",
                  "functionRef": {
                      "refName": "debug",
                      "arguments": {
                          "message": "Hello"
                      }
                  }
              }
          ],
          "transition": "PrintResult"
      },
        {
            "name": "PrintResult",
            "type": "operation",
            "actionMode": "sequential",
            "stateDataFilter": {
              "input": "{wfOptions: .wfCollection.name}"
            },
            "actions": [
                {
                    "name": "printAction",
                    "functionRef": {
                        "refName": "debug",
                        "arguments": {
                            "message": ".wfOptions"
                        }
                    }
                }
            ],
            "end": true
        }
    ]
}